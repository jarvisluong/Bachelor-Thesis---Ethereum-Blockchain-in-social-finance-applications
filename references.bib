@article{RefWorks:doc:NewProofOfWorkForBitcoin,
	author={Ning Shi},
	year={2016},
	title={A new proof-of-work mechanism for bitcoin},
	journal={Financial Innovation},
	volume={2},
	number={1},
	pages={1-8},
